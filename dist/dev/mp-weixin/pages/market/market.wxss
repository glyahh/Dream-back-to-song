
.page.data-v-d5ebafbc {
  background-color: #f6f2e9;
  min-height: 100vh;
  padding-bottom: 150rpx;
  box-sizing: border-box;
  font-family: 'PingFang SC', 'Noto Sans SC', system-ui, -apple-system, sans-serif;
}

/* 顶部大图区域 */
.hero.data-v-d5ebafbc {
  position: relative;
  height: 680rpx;
  overflow: hidden;
}
.hero-swiper.data-v-d5ebafbc {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 680rpx;
}
.hero-swiper swiper-item.data-v-d5ebafbc {
  height: 680rpx;
}
.hero-swiper-item.data-v-d5ebafbc {
  width: 100%;
  height: 100%;
}
.hero-img.data-v-d5ebafbc {
  width: 100%;
  height: 100%;
  display: block;
}
.search-bar.data-v-d5ebafbc {
  position: absolute;
  top: 60rpx;
  left: 30rpx;
  right: 100rpx;
  height: 80rpx;
  background-color: rgba(255, 255, 255, 0.95);
  border-radius: 50rpx;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
  opacity: 0;
  transform: translateY(-20rpx);
  transition: opacity 0.6s ease 0.2s, transform 0.6s ease 0.2s;
}
.search-bar.animate-in.data-v-d5ebafbc {
  opacity: 1;
  transform: translateY(0);
}
.search-icon.data-v-d5ebafbc {
  font-size: 32rpx;
  color: #b6b6b6;
  margin-right: 16rpx;
  line-height: 1;
}
.search-input.data-v-d5ebafbc {
  flex: 1;
  font-size: 28rpx;
  color: #333;
}
.search-placeholder.data-v-d5ebafbc {
  color: #b6b6b6;
}
.cart-icon.data-v-d5ebafbc {
  position: absolute;
  top: 70rpx;
  right: 40rpx;
  font-size: 44rpx;
  color: #666;
}
.hero-dots.data-v-d5ebafbc {
  position: absolute;
  bottom: 30rpx;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  align-items: center;
  gap: 16rpx;
}
.dot.data-v-d5ebafbc {
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background-color: #ddd4c5;
}
.dot.active.data-v-d5ebafbc {
  background-color: #9ea97f;
}

/* 快捷入口 */
.quick-nav.data-v-d5ebafbc {
  display: flex;
  justify-content: space-around;
  padding: 40rpx 30rpx;
  background-color: #edece7;
  opacity: 0;
  transform: translateY(30rpx);
  transition: opacity 0.8s ease 0.3s, transform 0.8s ease 0.3s;
}
.quick-nav.animate-in.data-v-d5ebafbc {
  opacity: 1;
  transform: translateY(0);
}
.nav-item.data-v-d5ebafbc {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 16rpx;
}
.nav-icon-wrapper.data-v-d5ebafbc {
  width: 100rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav-icon-img.data-v-d5ebafbc {
  width: 80rpx;
  height: 80rpx;
}
.nav-text.data-v-d5ebafbc {
  font-size: 26rpx;
  color: #333;
}

/* H5悬停效果 */
@media (hover: hover) {
.nav-item.data-v-d5ebafbc {
    transition: transform 0.3s ease;
}
.nav-item.data-v-d5ebafbc:hover {
    transform: translateY(-4rpx);
}
.nav-icon-wrapper.data-v-d5ebafbc {
    transition: transform 0.3s ease;
}
.nav-item:hover .nav-icon-wrapper.data-v-d5ebafbc {
    transform: scale(1.1);
}
.nav-text.data-v-d5ebafbc {
    transition: color 0.3s ease;
}
.nav-item:hover .nav-text.data-v-d5ebafbc {
    color: #9ea97f;
}
}

/* 商品区域 */
.section.data-v-d5ebafbc {
  padding: 30rpx;
}
.section-header.data-v-d5ebafbc {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 24rpx;
  opacity: 0;
  transform: translateY(20rpx);
  transition: opacity 0.6s ease, transform 0.6s ease;
}
.section-header.animate-in.data-v-d5ebafbc {
  opacity: 1;
  transform: translateY(0);
}
.section-title.data-v-d5ebafbc {
  font-size: 34rpx;
  color: #333;
  font-weight: 600;
}
.section-arrow.data-v-d5ebafbc {
  font-size: 32rpx;
  color: #999;
}
.product-grid.data-v-d5ebafbc {
  display: flex;
  gap: 20rpx;
  opacity: 0;
  transform: translateY(30rpx);
  transition: opacity 0.8s ease 0.2s, transform 0.8s ease 0.2s;
}
.product-grid.animate-in.data-v-d5ebafbc {
  opacity: 1;
  transform: translateY(0);
}
.product-card.data-v-d5ebafbc {
  flex: 1;
  background-color: #fff;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 0 4rpx 16rpx rgba(0, 0, 0, 0.08);
  will-change: opacity;
}
.product-card-img-wrapper.data-v-d5ebafbc {
  width: 100%;
  height: 280rpx;
  overflow: hidden;
}
.product-card-img.data-v-d5ebafbc {
  width: 100%;
  height: 100%;
}
.product-card-name.data-v-d5ebafbc {
  display: block;
  padding: 20rpx;
  font-size: 28rpx;
  color: #333;
  text-align: center;
  font-weight: 500;
}

/* H5悬停效果 */
@media (hover: hover) {
.product-card.data-v-d5ebafbc {
    transition: transform 0.35s cubic-bezier(0.34, 1.56, 0.64, 1), box-shadow 0.35s ease;
}
.product-card.data-v-d5ebafbc:hover {
    transform: translateY(-8rpx) scale(1.02);
    box-shadow: 0 20rpx 50rpx rgba(0, 0, 0, 0.15);
}
.product-card:hover .product-card-img.data-v-d5ebafbc {
    transform: scale(1.08);
}
.product-card:hover .product-card-name.data-v-d5ebafbc {
    color: #9ea97f;
}
.product-card-img.data-v-d5ebafbc {
    transition: transform 0.4s ease;
}
.product-card-name.data-v-d5ebafbc {
    transition: color 0.3s ease;
}
}

/* 新品推荐 */
.new-products-banner.data-v-d5ebafbc {
  width: 100%;
  height: 200rpx;
  border-radius: 20rpx;
  overflow: hidden;
}
.new-products-img.data-v-d5ebafbc {
  width: 100%;
  height: 100%;
}

/* 底部导航栏 */
.tabbar.data-v-d5ebafbc {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 140rpx;
  background-color: #fbf8f2;
  border-top: 1rpx solid rgba(0, 0, 0, 0.08);
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding-bottom: env(safe-area-inset-bottom);
  box-shadow: 0 -6rpx 20rpx rgba(0, 0, 0, 0.06);
  z-index: 1000;
}
.tab-item.data-v-d5ebafbc {
  flex: 1;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #b0b0b0;
  font-size: 22rpx;
  letter-spacing: 2rpx;
}
.tab-item.active.data-v-d5ebafbc {
  color: #9ea97f;
}
.tab-icon.data-v-d5ebafbc {
  width: 54rpx;
  height: 54rpx;
  margin-bottom: 8rpx;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
  opacity: 0.85;
}
.tab-item.active .tab-icon.data-v-d5ebafbc {
  opacity: 1;
}

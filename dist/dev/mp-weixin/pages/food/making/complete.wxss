  color: black;
  letter-spacing: 8rpx;
  font-family: 'LiSu', 'SimLi', serif;
  flex: 1;
  text-align: center;
  margin-left: -40rpx;
  margin-top: 130rpx;
}
.right-actions.data-v-61a885d5 {
  width: 80rpx;
}

/* 主内容区 */
.main-content.data-v-61a885d5 {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 60rpx 30rpx 100rpx;
}
.complete-title.data-v-61a885d5 {
  margin-bottom: 40rpx;
}
.title-text.data-v-61a885d5 {
  font-size: 60rpx;
  color: #5a4a42;
  font-family: 'LiSu', 'SimLi', serif;
